generator client {
  provider = "prisma-client-js"
  output   = "./src/prisma/client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model SearchNumber {
  id String @id() @default(uuid())
  key_value String @unique
  count_unit String
  compite_result Boolean
  create_data DateTime @default(now())
  update_data DateTime @updatedAt
}
